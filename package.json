    "ttf2woff": "^1.3.0",
    "ttfinfo": "^0.2.0"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "cssify": "^0.7.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
